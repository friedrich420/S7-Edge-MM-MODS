    <com.android.settings.PreferenceWithLongSummary android:title="@string/omc_version" android:key="omc_version" android:summary="@string/device_info_default" style="?android:preferenceInformationStyle" />
    <Preference android:title="@string/security_patch" android:key="security_patch" android:summary="@string/device_info_default" style="?android:preferenceInformationStyle" />
</PreferenceScreen>